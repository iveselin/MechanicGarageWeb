extends ../layout

block content 
    .container-fluid.text-center
        .row.content
            include ../includes/left_sidebar

            .col-sm-8.text-left
                each file, i in data
                    a(href="/administration/requests/"+file.object_id) Data nr. #{i} #{file.extraNote} #{file.object_id}
                    hr  
                else
                    h3 No data
                    
            include ../includes/right_sidebar
        